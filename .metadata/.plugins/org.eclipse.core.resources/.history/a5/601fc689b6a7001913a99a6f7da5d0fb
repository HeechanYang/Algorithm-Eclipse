package datastructures.trees;

public class BinarySearchTree {
	private Node root;
	
	public BinarySearchTree() {
		super();
		this.root = null;
	}
	
	public void add(int num) {
		
	}

	class Node{
		private int data;
		private Node left;
		private Node right;
		
		public Node(int data) {
			super();
			this.data = data;
			this.left = null;
			this.right = null;
		}
	}
}
